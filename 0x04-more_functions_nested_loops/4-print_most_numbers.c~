#include<stdio.h>
#include "main.h"
/**
 * print_numbers - the entry function
 * Description: 'Program uses the function _putchar to print numbers (0 to 9)
 * Except 2 and 4'
 * return : Always 0 (SUCCESS)
 */
void print_most_numbers(void)
{
int a;

for (a = 0; a <= 9; a++)
{
if (a != 2 && a != 4)
{
_putchar('0' + a);
}
}
_putchar('\n');
}

