#include <stdlib.h>
#include <time.h>
/** 
 * main - the entry function
 *
 * Return: Always 0 (SUCESS)
 */

/* more headers goes there */
/* betty style doc for function main goes there */
int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	/* your code goes there */
	If (n < 0)
	  printf("is negative");
	else if (n == 0)
	  printf("is 0");
	else
	  printf("is positive");
	return (0);
}
